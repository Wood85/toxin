include ../logo-block/logo-block

-
  const menuValues = ['О нас', 'Услуги', 'Вакансии', 'Новости', 'Соглашения']

header.header
  .header__container
    .header__logo
      +logoBlock
    nav.header__menu
      ul.header__menu-list
        each value in menuValues
          li.header__menu-item
            a.header__menu-link(href="/mock-address/change-me") #{value}
      .header__authentication-block
        a.header__profile(href="/mock-address/change-me") Юлий Цезарь