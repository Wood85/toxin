mixin logoBlock
  .logo-block
    a.logo-block__image(href="/mock-address/change-me")
      img.logo-block__image-img(src="./assets/img/header-logo-image.svg" alt="logo")
    a.logo-block__title(href="/mock-address/change-me")
      img.logo-block__title-img(src="./assets/img/header-logo-title.svg" alt="title")